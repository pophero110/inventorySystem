
<div class="pl-2 pr-2">

<div class="d-flex justify-content-between align-items-center">
<h1>Products List</h1>
<%= link_to "Back to homepage", root_path, class: "btn btn-outline-secondary" %>
</div>

<div class="row">
  <% @product_lists.each do |productList| %>
    <div class="card text-center col-3">
  <div class="card-body">
    <h5 class="card-title"><%= productList.created_at %></h5>
     <p class="card-text"><%= link_to "[View data]", product_list_path(productList) %></p>


  </div>
  </div>
<% end %>
</div>

</div>
</div>
